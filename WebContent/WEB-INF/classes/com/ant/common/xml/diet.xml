<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"     
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="diet">
	
	 <!-- 회비 환경설정 검색 -->
	 <select id="dietlist" resultClass="java.util.HashMap">
		select out.*
		from ( 	
			select
				row_number() over (order by qa_serial_no) ncount,
				a.*
			from QA_Board a
		) out	   
		where 1=1 
			and out.ncount > #nstart#
			and #nend# >= out.ncount
	</select> 	
				
	 <!-- 회비 환경설정 Count --> 	
	 <select id="dietlistcnt" resultClass="java.util.HashMap">
		select count(*) ncount from QA_Board
	 </select>
	
</sqlMap>    