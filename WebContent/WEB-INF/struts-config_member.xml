<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">


<struts-config>

	<!-- Form Bean Definitions -->
    <form-beans></form-beans> 

	<!-- Action Mapping Definitions -->
  	<action-mappings>
  	
  	
		<!-- 2.0 Home>회원 left 메뉴 출력			--><!-- 김성훈 2012.09.12 -->
		<action path="/memberForm"		forward=".layout_member"/>	
				

		<action path="/memberInsert"	type="com.ant.member.insert.action.memberInsertAction" parameter="method" scope="request">
			<forward name="memberInsertList"	path=".layout_memberInsert"				/>	<!-- 김성훈 2012.10.10 : 2.1 Home>회원>회원 등록				-->
			<forward name="sLicnoCnt"			path="/jsp/member/insert/sLicnoCnt.jsp"	/>	<!-- 김성훈 2012.10.16 : 			라이센스 중복검사			-->
			<forward name="postSearchr" 		path="/jsp/member/insert/post.jsp"		/>	<!-- 김성훈 2012.10.16 : 			기본정보 우편번호 검색		-->
			<forward name="compostSearchr"		path="/jsp/member/insert/compost.jsp"	/>	<!-- 김성훈 2012.10.16 : 			근무처정보 우편번호 검색	-->
			<forward name="idChkr"				path="/jsp/member/insert/idChk.jsp"	/>
		</action>
		
		<action path="/memberSearch"	type="com.ant.member.search.action.memberSearchAction" parameter="method" scope="request">
			<forward name="memberSearchList"	path=".layout_memberSearch" 			/>	<!-- 김성훈 2012.09.17 : 2.2 Home>회원>조건 검색				-->
			<forward name="pers_check"			path="/jsp/member/pers_check.jsp"		/>	<!-- 김성훈 2012.11.14 : 			엑셀 저장	-->
			<forward name="umsDataall"          path="/jsp/member/umsdataForm.jsp"		/>	<!-- 김성훈 2012.11.15 : 			메일 보내기 화면	-->
			<forward name="sendDMForm"     		path="/jsp/member/sendDMForm.jsp"		/>	<!-- 김성훈 2012.11.27 : 			DM발송 출력			-->
			<forward name="sendMsgForm" 		path="/jsp/member/sendMsgForm.jsp"		/>	<!-- 김성훈 2012.11.27 : 			쪽지 보내기 폼	-->
			<forward name="sendUmsForm" 		path="/jsp/member/sendUmsForm.jsp"		/>	<!-- 김성훈 2012.11.28 : 			문자 보내기 폼	-->
			<forward name="sendAllLink" 		path="/jsp/member/sendAllLink.jsp"		/>	<!-- 김성훈 2012.11.28 : 			DM 발송, 쪽지, 문자 보내기 결과링크	-->

		</action>
		
		<action path="/memberDues"	type="com.ant.member.dues.action.memberDuesAction" parameter="method" scope="request">
			<forward name="memberDuesList"		path=".layout_memberDues" 				/>	<!-- 김성훈 2012.09.26 : 2.3 Home>회원>회비 처리				-->
			<forward name="pers_check"			path="/jsp/member/pers_check.jsp"		/>	<!-- 김성훈 2012.11.29 : 			엑셀 저장	-->
			<forward name="umsDataall"          path="/jsp/member/umsdataForm.jsp"		/>	<!-- 김성훈 2012.11.15 : 			메일 보내기 화면(전체,개별은 그자리에서 바로 보냄)	-->
			<forward name="membershipForm"     	path="/jsp/member/membershipForm.jsp"	/>	<!-- 김성훈 2012.11.26 : 			회원증 출력			-->
			<forward name="membershipLink"     	path="/jsp/member/membershipLink.jsp"	/>	<!-- 김성훈 2012.11.26 : 			회원증 출력 링크 포워딩	-->
			<forward name="sendDMForm"     		path="/jsp/member/sendDMForm.jsp"		/>	<!-- 김성훈 2012.11.27 : 			DM발송 출력			-->
			<forward name="sendUmsForm" 		path="/jsp/member/sendUmsForm.jsp"		/>	<!-- 김성훈 2012.11.28 : 			문자 보내기 폼	-->
			<forward name="sendAllLink" 		path="/jsp/member/sendAllLink.jsp"		/>	<!-- 김성훈 2012.11.28 : 			DM 발송, 문자 보내기 결과링크	-->

		</action>
		
		<action path="/memberDeposit"	type="com.ant.member.deposit.action.memberDepositAction" parameter="method" scope="request">
			<forward name="memberDepositList"	path=".layout_memberDeposit"			/>	<!-- 김성훈 2012.09.26 : 2.4 Home>회원>예수금 현황 		-->
			<forward name="pers_check"			path="/jsp/member/pers_check.jsp"		/>	<!-- 김성훈 2012.11.20 : 			엑셀 저장	-->
		</action>
		
		<action path="/memberFund"	type="com.ant.member.fund.action.memberFundAction" parameter="method" scope="request">
			<forward name="memberFundList"		path=".layout_memberFund"				/><!-- 김성훈 2012.10.20 : 2.5 Home>회원>기부/기금 현황 		-->
			<forward name="pers_check"			path="/jsp/member/pers_check.jsp"		/><!-- 김성훈 2012.11.14 : 			엑셀 저장	-->
		</action>
			
		<!-- 2.6 회원>현황 메뉴 진행 -->
		<action path="/memberState" type="com.ant.member.state.action.memberStateAction" parameter="method" scope="request">
			<forward name="memberState"			path=".layout_member"			/><!-- 김성훈 2012.09.12 : 2.6.0 Home>회원>현황(메뉴출력) -->
			
			<forward name="bigStateList"		path=".layout_bigState"			/><!-- 박상모 2012.10.15 : 지부별회원-분과별 -->
			<forward name="smallStateList"		path=".layout_smallState" 		/><!-- 박상모 2012.10.15 : 지부별회원-소분류별 -->
			<forward name="memGubunStateList"	path=".layout_memGubunState"	/><!-- 박상모 2012.10.15 : 지부별회원-회원종류별 -->
			<forward name="companyStateList"	path=".layout_companyState"		/><!-- 김성훈 2012.09.10 : 2.6.2 Home>회원>현황>근무처분류별 -->

			<forward name="trustComStateList"	path=".layout_trustComState" 	/><!-- 박상모 2012.10.09 : 위탁업체 업체별 -->
			<forward name="jLevelStateList"		path=".layout_jLevelState" 		/><!-- 박상모 2012.10.09 : 고용형태별(직급) -->
			<forward name="jLineStateList"		path=".layout_jLineState" 		/><!-- 박상모 2012.10.09 : 고용형태별(직렬) -->
			<forward name="certifiStateList"	path=".layout_certifiState" 	/><!-- 박상모 2012.10.09 : 고용형태별(직렬) -->
			<forward name="univerStateList"		path=".layout_univerState" 		/><!-- 박상모 2012.10.09 : 최종학위별 -->
			<forward name="cookStateList"		path=".layout_cookState" 		/><!-- 박상모 2012.10.09 : 공동조리 -->
			<forward name="conStateList"		path=".layout_conState" 		/><!-- 박상모 2012.10.09 : 공동관리 -->
			
			<forward name="codeUseMemberList"	path=".layout_codeUseMember"	/><!-- 이정기 2012.11.12 : 2.6.03 Home>회원>현황>운영 형태별 -->
			<forward name="subCodePersonList"	path=".layout_subCodePerson"	/><!-- 윤석희 2012.11.12 : 2.6.04 Home>회원>현황>위탁업체 개인별 -->
			
			<forward name="subCodeStateList"	path=".layout_subCodeState" 	/><!-- 김성훈 2012.09.13 : 2.6.12 Home>회원>현황>산하단체 소속별 -->
			<forward name="mealStateList"		path=".layout_mealState" 		/><!-- 김성훈 2012.11.10 : 2.6.13 Home>회원>현황>피 급식자별 -->
			<forward name="yearPayStateList"	path=".layout_yearPayState" 	/><!-- 김성훈 2012.11.10 : 2.6.14 Home>회원>현황>영양사 연봉별 -->
			<forward name="mealEmployStateList"	path=".layout_mealEmployState"	/><!-- 김성훈 2012.11.10 : 2.6.15 Home>회원>현황>급식 종사자별 -->
			<forward name="licPayStateList"		path=".layout_licPayState" 		/><!-- 김성훈 2012.11.10 : 2.6.16 Home>회원>현황>영양사 면허수당별	-->
			<forward name="companyMealStateList" path=".layout_companyMealState"/><!-- 김성훈 2012.11.11 : 2.6.17 Home>회원>현황>1식 재료비별 -->
			<forward name="multyStateList"		path=".layout_multyState" 		/><!-- 김성훈 2012.11.11 : 2.6.18 Home>회원>현황>겸직별 -->
			<forward name="mealNumberStateList"	path=".layout_mealNumberState" 	/><!-- 김성훈 2012.11.11 : 2.6.19 Home>회원>현황>급식인원별 -->		
		</action>
		
    </action-mappings>	
</struts-config>
